{
    "defaultSeverity": "INFO",
    "checks": [
	{
	    "type": "Anonymous",
	    "props": {
		"severity": "ERROR"
	    }
	},
	{
	    "type": "ArrayAccess"
	},
	{
	    "type": "ArrayLiteral",
	    "props": {
		"severity": "WARNING"
	    }
	},
	{
	    "type": "AvoidStarImport"
	},
	{
	    "type": "CatchParameterName",
	    "props": {
		"format": "^(e)$"
	    }
	},
	{
	    "type": "ConstantName",
	    "props": {
		"severity": "ERROR",
		"format": "^[A-Z][A-Z0-9]*(_[A-Z0-9]+)*$",
		"tokens": [
		    "INLINE",
		    "NOTINLINE"
		]
	    }
	},
	{
	    "type": "CyclomaticComplexity",
	    "props": {
		"thresholds": [
		    {
			"severity": "INFO",
			"complexity": 15
		    },
		    {
			"severity": "WARNING",
			"complexity": 18
		    },
		    {
			"severity": "ERROR",
			"complexity": 21
		    }
		]
	    }
	},
	{
	    "type": "DefaultComesLast"
	},
	{
	    "type": "EmptyBlock"
	},
	{
	    "type": "EmptyLines"
	},
	{
	    "type": "EmptyPackage"
	},
	{
	    "type": "ERegLiteral",
	    "props": {
		"severity": "WARNING"
	    }
	},
	{
	    "type": "FileLength",
	    "props": {
		"severity": "WARNING",
		"max": 1000
	    }
	},
	{
	    "type": "HexadecimalLiteral"
	},
	{
	    "props": {
		"ignoreSetter": true,
		"ignoreFormat": "^(main|run)$",
		"ignoreConstructorParameter": true,
		"severity": "INFO"
	    },
	    "type": "HiddenField"
	},
	{
	    "type": "InnerAssignment"
	},
	{
	    "props": {
		"allowProperties": false,
		"allowMarkerInterfaces": true
	    },
	    "type": "Interface"
	},
	{
	    "type": "LeftCurly",
	    "props": {
		"severity": "WARNING",
		"option": "eol",
		"tokens": [
		    "CLASS_DEF",
		    "ENUM_DEF",
		    "ABSTRACT_DEF",
		    "TYPEDEF_DEF",
		    "CLASS_DEF",
		    "INTERFACE_DEF",
		    "FUNCTION",
		    "FOR",
		    "IF",
		    "WHILE",
		    "SWITCH",
		    "TRY",
		    "CATCH"
		]
	    }
	},
	{
	    "type": "LineLength",
	    "props": {
		"severity": "WARNING",
		"max": 160
	    }
	},
	{
	    "type": "LocalVariableName",
	    "props": {
		"severity": "ERROR",
		"format": "^[a-z]+[a-zA-Z0-9]*$"
	    }
	},
	{
	    "type": "MemberName",
	    "props": {
		"severity": "ERROR",
		"format": "^[A-Z][A-Z0-9]*(_[A-Z0-9]+)*$",
		"tokens": [
		    "ENUM"
		]
	    }
	},
	{
	    "type": "MemberName",
	    "props": {
		"severity": "ERROR",
		"format": "^[a-z]+[a-zA-Z0-9]*$",
		"tokens": [
		    "PUBLIC",
		    "TYPEDEF"
		]
	    }
	},
	{
	    "type": "MemberName",
	    "props": {
		"severity": "ERROR",
		"format": "^_[a-z]+[a-zA-Z0-9]*$",
		"tokens": [
		    "PRIVATE"
		]
	    }
	},
	{
	    "type": "MethodLength",
	    "props": {
		"severity": "WARNING",
		"max": 100
	    }
	},
	{
	    "type": "MethodName",
	    "props": {
		"severity": "ERROR",
		"format": "^_[a-z]+[a-zA-Z0-9]*$",
		"tokens": [
		    "PRIVATE",
		    "NOTSTATIC"
		]
	    }
	},
	{
	    "type": "MethodName",
	    "props": {
		"severity": "ERROR",
		"format": "^[a-z]+[a-zA-Z0-9]*$",
		"tokens": [
		    "PUBLIC"
		]
	    }
	},
	{
	    "type": "MultipleStringLiterals",
	    "props": {
		"allowDuplicates": 2,
		"minLength": 2
	    }
	},
	{
	    "type": "MultipleVariableDeclarations"
	},
	{
	    "type": "NeedBraces",
	    "props": {
		"severity": "WARNING",
		"allowSingleLineStatement": true,
		"tokens": [
		    "IF",
		    "ELSE_IF",
		    "FOR",
		    "WHILE"
		]
	    }
	},
	{
	    "type": "NestedForDepth",
	    "props": {
		"severity": "ERROR",
		"max": 2
	    }
	},
	{
	    "type": "NestedIfDepth",
	    "props": {
		"severity": "ERROR",
		"max": 2
	    }
	},
	{
	    "type": "NestedTryDepth",
	    "props": {
		"severity": "ERROR",
		"max": 2
	    }
	},
	{
	    "type": "ParameterName",
	    "props": {
		"severity": "ERROR",
		"format": "^[a-z][a-zA-Z0-9]*$"
	    }
	},
	{
	    "type": "ParameterNumber",
	    "props": {
		"severity": "WARNING",
		"max": 7,
		"ignoreOverriddenMethods": false
	    }
	},
	{
	    "type": "RedundantModifier"
	},
	{
	    "type": "Return",
	    "props": {
		"allowEmptyReturn": true
	    }
	},
	{
	    "type": "RightCurly",
	    "props": {
		"severity": "WARNING",
		"option": "aloneorsingle",
		"tokens": [
		    "CLASS_DEF",
		    "ENUM_DEF",
		    "ABSTRACT_DEF",
		    "TYPEDEF_DEF",
		    "CLASS_DEF",
		    "INTERFACE_DEF",
		    "OBJECT_DECL",
		    "FUNCTION",
		    "FOR",
		    "IF",
		    "WHILE",
		    "SWITCH",
		    "TRY",
		    "CATCH"
		]
	    }
	},
	{
	    "type": "SimplifyBooleanExpression"
	},
	{
	    "type": "SimplifyBooleanReturn"
	},
	{
	    "type": "Spacing",
	    "props": {
		"spaceIfCondition": true,
		"spaceAroundBinop": true,
		"spaceAroundBinop": true,
		"ignoreRangeOperator": true
	    }
	},
	{
	    "props": {
		"allowException": true,
		"policy": "doubleAndInterpolation"
	    },
	    "type": "StringLiteral"
	},
	{
	    "type": "TODOComment"
	},
	{
	    "type": "TabForAligning"
	},
	{
	    "type": "TrailingWhitespace"
	},
	{
	    "type": "Type",
	    "props": {
		"severity": "ERROR"
	    }
	},
	{
	    "type": "TypeName",
	    "props": {
		"severity": "ERROR",
		"format": "^[A-Z]+[a-zA-Z0-9]*$",
		"tokens": [
		    "CLASS",
		    "ENUM",
		    "TYPEDEF"
		]
	    }
	},
	{
	    "type": "TypeName",
	    "props": {
		"severity": "ERROR",
		"format": "^I[A-Z]+[a-zA-Z0-9]*$",
		"tokens": [
		    "INTERFACE"
		]
	    }
	},
	{
	    "type": "UnnecessaryConstructor"
	},
	{
	    "type": "VariableInitialisation",
	    "props": {
		"severity": "ERROR"
	    }
	}
    ]
}